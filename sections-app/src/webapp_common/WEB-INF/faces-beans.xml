<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	<managed-bean>
		<description>Sample Bean</description>
		<managed-bean-name>sampleBean</managed-bean-name>
		<managed-bean-class>org.sakaiproject.tool.sections.jsf.beans.SampleBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<description>Sample Manager</description>
			<property-name>sampleManager</property-name>
			<value>#{org_sakaiproject_api_sections_SampleManager}</value>
		</managed-property>
		<managed-property>
			<description>Authn</description>
			<property-name>authn</property-name>
			<value>#{org_sakaiproject_api_sections_facades_Authn}</value>
		</managed-property>
	</managed-bean>
</faces-config>
